{
  "name": "lis-report-app",
  "version": "0.1.0",
  "description": "A minimal Electron application",
  "main": "app.js",
  "scripts": {
    "start": "electron app.js",
    "package": "set \"ELECTRON_MIRROR=http://npm.taobao.org/mirrors/electron/\" && electron-packager ./ lis-report-app --platform=win32 --arch=ia32 --version=0.36.4 --out=./dist --overwrite=true --ignore=node_modules/electron-* --ignore=node_modules/.bin --ignore=.git --ig nore=dist --prune=true"
  },
  "repository": "https://github.com/appleshan/lis-report-app",
  "author": "appleshan",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^1.7.11",
    "electron-packager": "^10.1.0",
    "electron-prebuilt": "^1.4.13"
  }
}
